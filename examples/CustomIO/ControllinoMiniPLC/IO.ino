void customIO() {
  // Input pin directions
  pinMode(AN0, INPUT);
  pinMode(AN1, INPUT);
  pinMode(AN2, INPUT);
  pinMode(AN3, INPUT);
  pinMode(AN4, INPUT);
  pinMode(AN5, INPUT);

  
  // Output pin directions
  pinMode(D0, OUTPUT);
  pinMode(D1, OUTPUT);
  pinMode(D2, OUTPUT);
  pinMode(D3, OUTPUT);
  pinMode(D4, OUTPUT);
  pinMode(D5, OUTPUT);
  pinMode(D6, OUTPUT);
  pinMode(D7, OUTPUT);
}

