void customIO() {
  // Input pin directions
  // B Zone inputs
  pinMode(I00, INPUT);
  pinMode(I01, INPUT);
  pinMode(I02, INPUT);
  pinMode(I03, INPUT);
  pinMode(I04, INPUT);
  pinMode(I05, INPUT);

  // Output pin directions
  // B Zone outputs
  pinMode(Q00, OUTPUT);
  pinMode(Q01, OUTPUT);
  pinMode(Q02, OUTPUT);
  
  // B Zone Relay Outputs
  pinMode(R01, OUTPUT);
  pinMode(R02, OUTPUT);
  pinMode(R03, OUTPUT);
  pinMode(R04, OUTPUT);
  pinMode(R05, OUTPUT);
  pinMode(R06, OUTPUT);
  pinMode(R07, OUTPUT);
  pinMode(R08, OUTPUT);
}

